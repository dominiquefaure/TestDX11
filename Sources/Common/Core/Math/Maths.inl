
//-------------------------------------------------------------------------
FORCE_INLINE float Maths::Abs( float a_value )
{
	return fabsf( a_value );
}
//-------------------------------------------------------------------------

//-------------------------------------------------------------------------
FORCE_INLINE float Maths::Cos( float a_angle )
{
	return cosf( a_angle );
}
//-------------------------------------------------------------------------

//-------------------------------------------------------------------------
FORCE_INLINE float Maths::Sin( float a_angle )
{
	return sinf( a_angle );
}
//-------------------------------------------------------------------------

//-------------------------------------------------------------------------
FORCE_INLINE float Maths::Tan( float a_angle )
{
	return tanf( a_angle );
}
//-------------------------------------------------------------------------
